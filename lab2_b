#include<stdio.h>

int Fibo(int n){
   
    static int a = 0, b = 1, nextTerm;    
    if(n > 0)
    {    
        nextTerm = a + b;    
        a = b;    
        b = nextTerm;    
   
        printf("%d, ",nextTerm);
        Fibo(n-1);    
    }
}

int main()
{
    int n = 10;
    printf("0, 1, ");
    Fibo(n-2);
    return 0;
}
